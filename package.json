    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "3",
    "typescript-eslint": "^8.28.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {}
